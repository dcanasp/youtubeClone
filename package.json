{
  "name": "ytclone",
  "version": "1.0.0",
  "description": "a backend youtube clone",
  "main": "index.js",
  "scripts": {
    "init": "npm i && tsc",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "migrateDb": "npx sequelize-auto -l ts -o models -d ytClone -u postgres -x Colegio5 -h 127.0.0.1 -p 5432 -e postgres",
    "start": "node build/src/server.js",
    "singleChange": "ts-node src/server.ts",
    "build": "tsc",
    "production": "tsc && node build/src/server.js",
    "startQueue": "docker run -d --name some-rabbit -p 5672:5672 -p 15672:15672 -e RABBITMQ_DEFAULT_USER=ytClone -e RABBITMQ_DEFAULT_PASS=password rabbitmq:management",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.474.0",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^6.0.1",
    "amqplib": "^0.10.3",
    "argon2": "^0.31.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.16.2",
    "pino-multi-stream": "^6.0.0",
    "sequelize": "^6.35.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "@types/pino-multi-stream": "^5.1.6",
    "@types/pino-pretty": "^5.0.0",
    "@types/uuid": "^9.0.7",
    "pino-pretty": "^10.2.3",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
